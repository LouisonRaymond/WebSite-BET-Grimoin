<div class="col-md-6 col-lg-4 wow fadeInUp" data-wow-delay="<%= delay %>">
    <div class="service-item">
      <div class="overflow-hidden">
        <img
          class="img-fluid"
          src="<%= project.picture %>"
          alt="image du batiment"
        />
      </div>
      <div
        class="p-4 text-center border border-5 border-light border-top-0"
      >
        <h4 class="mb-3"><%= project.title %></h4>
        <%- md(project.shortDescription) %>
        <a class="fw-medium" href="<%= `/${locale}/projet/${project.id}-${project.title}` %>"
          >Voir plus<i class="fa fa-arrow-right ms-2"></i
        ></a>
      </div>
    </div>
  </div>