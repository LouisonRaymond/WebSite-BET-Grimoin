<!DOCTYPE html>
<html lang="fr">
  <%- include('partials/head', { model: { locale: model.locale, title: 'BET GRIMOIN - Expertises' } }) %>
  <%- include('partials/header', { model: { locale: model.locale, translations: model.translations } }) %>


  <body>
    <!-- Hero Start -->
    <section id="fh5co-features">
      <img
        src="/img/banner contact good.png"
        style="width: 100%; margin-top: -13px"
      />
      <!-- Hero End -->
      <div class="container">
        <br />
        <div class="container feature py-5 px-lg-0">
          <div>
            <h1 class="text-center mb-5 wow fadeInUp" data-wow-delay="0.1s">
              <%= model.translations['expertises.title'] %>
            </h1>
            <div>
              <hr class="red_ligne" size="4px" />
            </div>
          </div>
        </div>
      </div>
    </section>
    <!-- Expertise Start -->
    <body>
      <div class="container mt-n5">
        <% model.expertises.forEach((expertise, index) => { %>
          <% if (index % 2 === 0) { %>
            <%- include('partials/expertise-bloc-left', { model: { expertise, locale: model.locale, translations: model.translations } }) %>
          <% } else { %>
            <%- include('partials/expertise-bloc-right', { model: { expertise, locale: model.locale, translations: model.translations } }) %>
          <% } %>
        <% }) %>
      </div>
    </body>
    <br /><br /><br /><br /><br /><br />
    <!-- Expertise End -->
    <%- include('partials/footer', { model: { locale: model.locale, translations: model.translations } }) %>
  </body>
</html>
